struct Aluno {
    float nota;
	char sexo;
	int periodo;
}
int nota (int periodo){
	int nota = 0;
	if (periodo == 3){
	   nota = +10;
	}
	return nota;
}

void imprimeAlunos() {
    /* imprime todoso os Alunos */
}

int main() {
	
	int x = 10;
	int i = 0;
	
	/* la√ßo para somar x = x + 10, 10 vezes */
	while (i < 10)  {
		x = x +10;
		i = i+ 1;
	}

	if(x >= 100){
		x = -10;
	} else {
        x = 15;
	}
	
	return 0;
}
